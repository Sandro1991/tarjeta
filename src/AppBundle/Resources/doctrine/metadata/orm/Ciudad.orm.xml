<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ciudad" table="ciudad">
    <indexes>
      <index name="IDX_8E86059ECD2D7CDD" columns="id_prov"/>
    </indexes>
    <id name="idCiud" type="integer" column="id_ciud">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nameCiud" type="string" column="name_ciud" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="code" type="integer" column="code" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idProv" target-entity="Provincia" fetch="LAZY">
      <join-columns>
        <join-column name="id_prov" referenced-column-name="id_prov"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
