<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Campeonato" table="campeonato">
    <indexes>
      <index name="IDX_722DB8CA402A7338" columns="id_comp"/>
    </indexes>
    <id name="idCamp" type="integer" column="id_camp">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nameCamp" type="string" column="name_camp" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechInic" type="date" column="fech_inic" nullable="true"/>
    <field name="fechFina" type="date" column="fech_fina" nullable="true"/>
    <field name="precInsc" type="integer" column="prec_insc" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="descCamp" type="string" column="desc_camp" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timestamp" type="datetime" column="timestamp" nullable="true"/>
    <field name="urlFoto" type="string" column="url_foto" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idComp" target-entity="Complejo" fetch="LAZY">
      <join-columns>
        <join-column name="id_comp" referenced-column-name="id_comp"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
