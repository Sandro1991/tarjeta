<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cancha" table="cancha">
    <indexes>
      <index name="IDX_9D09C782DF6812DC" columns="id_capa"/>
      <index name="IDX_9D09C782402A7338" columns="id_comp"/>
      <index name="IDX_9D09C782FB0D0145" columns="id_tipo"/>
    </indexes>
    <id name="idCanch" type="integer" column="id_canch">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nameCanch" type="string" column="name_canch" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cubi" type="boolean" column="cubi" nullable="true"/>
    <field name="precCanch" type="integer" column="prec_canch" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="timestamp" type="datetime" column="timestamp" nullable="true"/>
    <many-to-one field="idCapa" target-entity="Capacidad" fetch="LAZY">
      <join-columns>
        <join-column name="id_capa" referenced-column-name="id_capa"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idComp" target-entity="Complejo" fetch="LAZY">
      <join-columns>
        <join-column name="id_comp" referenced-column-name="id_comp"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipo" target-entity="Tcancha" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipo" referenced-column-name="id_tipo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
