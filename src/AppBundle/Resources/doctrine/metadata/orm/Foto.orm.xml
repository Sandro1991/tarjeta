<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Foto" table="foto">
    <indexes>
      <index name="id_camp" columns="id_camp"/>
      <index name="id_comp" columns="id_comp"/>
    </indexes>
    <id name="idFoto" type="integer" column="id_foto">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="imagename" type="string" column="imageName" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updatedat" type="datetime" column="updatedAt" nullable="true"/>
    <field name="timestamp" type="datetime" column="timestamp" nullable="true"/>
    <many-to-one field="idCamp" target-entity="Campeonato" fetch="LAZY">
      <join-columns>
        <join-column name="id_camp" referenced-column-name="id_camp"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idComp" target-entity="Complejo" fetch="LAZY">
      <join-columns>
        <join-column name="id_comp" referenced-column-name="id_comp"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
