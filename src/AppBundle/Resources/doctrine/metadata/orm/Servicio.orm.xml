<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Servicio" table="servicio">
    <id name="idServ" type="integer" column="id_Serv">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nameServ" type="string" column="name_serv" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="precServ" type="integer" column="prec_serv" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-many field="idComp" target-entity="Complejo" mapped-by="idServ" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
